    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "mini-css-extract-plugin": "^2.7.5",
    "pretty": "^2.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3",
    "webpack": "^5.79.0",
    "webpack-cli": "^5.1.1"
  },
  "dependencies": {
    "fs-extra": "^11.1.1",
    "mkdirp": "^3.0.1",
