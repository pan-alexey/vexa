  "main": "./dist/server/index.js",
  "types": "./dist/types/server/index.d.ts",
  "files": [
    "src",
    "shared",
    "module",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clear": "rimraf build",
    "dev": "yarn build:server && node ./__test__/index.js",
    "build": "yarn build:server && yarn build:types ",
    "build:server": "NODE_ENV=production webpack build --config webpack/webpack.server.ts",
    "build:types": "tsc --project tsconfig.dts.json --declaration --emitDeclarationOnly",
    "build:client": "NODE_ENV=production webpack build --config webpack/webpack.client.ts",
    "test": "jest --config jest.config.ts",
    "test:coverage": "jest --config jest.config.ts  --coverage",
    "lint": "eslint ./ --ext .js --ext .ts"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/eslint-parser": "^7.21.8",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "7.18.6",
    "@testing-library/dom": "^9.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.2",
    "@types/pretty": "^2.0.1",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/webpack": "^5.28.1",
