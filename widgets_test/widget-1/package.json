{
  "name": "@vexa/widget-1",
  "version": "1.0.0-dev",
  "private": true,
  "scripts": {
    "dev": "yarn run clear && yarn run build:client && yarn run build:server && npm run tar && npm run server",
    "tar": "tar -C dist -czvf dist.tgz ./ && cp dist.tgz dist",
    "server": "http-server ./dist -p 8081",
    "clear": "rimraf dist",
    "build:client": "NODE_ENV=production webpack build --config webpack/webpack.client.ts",
    "build:server": "NODE_ENV=production webpack build --config webpack/webpack.server.ts",
    "lint": "eslint ./ --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "yarn lint --fix"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "moment": "^2.29.4"
  },
  "devDependencies": {
    "http-server": "^14.1.1",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.2",
    "@types/react": "^18.0.15",
    "@types/semver": "^7.3.12",
    "@types/fs-extra":"^9.0.13",
    "@types/react-dom": "^18.0.6",
    "@types/webpack": "^5.28.0",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.3",
    "@testing-library/dom":"^8.17.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "clean-webpack-plugin": "^4.0.0",
    "babel-loader":"^8.2.5",
    "@babel/core":"^7.18.10",
    "@babel/preset-env":"^7.18.10",
    "@babel/preset-typescript":"^7.18.6",
    "@babel/preset-react":"^7.18.6",
    "mini-css-extract-plugin": "^2.6.1",
    "css-loader":"^6.7.1",
    "style-loader":"^3.3.1",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.6.1",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.10.1"
  }
}
